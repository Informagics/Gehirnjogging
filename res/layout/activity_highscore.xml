<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_highscore"
    android:orientation="vertical"
    tools:context=".Highscore" >

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" >

        <Button
            android:id="@+id/btn_Mathe"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="3.36"
            android:onClick="tabele_change"
            android:text="@string/Mathe" />

        <Button
            android:id="@+id/btn_BitOut"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="3.36"
            android:onClick="tabele_change"
            android:text="@string/BitOut" />

        <Button
            android:id="@+id/btn_Cable"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="3.36"
            android:onClick="tabele_change"
            android:text="@string/Kabelbinder" />

        <Button
            android:id="@+id/btn_Memory"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="3.36"
            android:onClick="tabele_change"
            android:text="@string/Memory" />

        <Button
            android:id="@+id/btn_Quiz"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="3.36"
            android:onClick="tabele_change"
            android:text="@string/Informatik" />
    </LinearLayout>

    <TextView
        android:id="@+id/txt_HS_Titel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="44dp"
        android:text="@string/Kabelbinder"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/txt_HS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txt_HS_Titel"
        android:layout_centerHorizontal="true"
        android:text="" />

</RelativeLayout>